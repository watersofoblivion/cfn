(* Project File *)

type project_file =
  { id: string }

(* Lock File *)

type vcs =
  [ Git <json name="git"> ]
<ocaml repr="classic">

type dependency =
  { source:  string;
    major:   int;
    vcs:     vcs;
    version: string wrap <ocaml t="Semver.t" wrap="Semver.of_string" unwrap="Semver.to_string">;
    commit:  string; }

type lock_file =
  { dependencies: dependency list }
